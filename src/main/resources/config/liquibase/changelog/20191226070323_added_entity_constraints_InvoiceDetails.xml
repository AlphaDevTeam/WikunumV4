<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity InvoiceDetails.
    -->
    <changeSet id="20191226070323-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="item_id"
                                 baseTableName="invoice_details"
                                 constraintName="fk_invoice_details_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="items"/>

        <addForeignKeyConstraint baseColumnNames="inv_id"
                                 baseTableName="invoice_details"
                                 constraintName="fk_invoice_details_inv_id"
                                 referencedColumnNames="id"
                                 referencedTableName="invoice"/>

    </changeSet>
</databaseChangeLog>
