<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ItemBinCard.
    -->
    <changeSet id="20191226061639-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="history_id"
                                 baseTableName="item_bin_card"
                                 constraintName="fk_item_bin_card_history_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_history"/>

        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="item_bin_card"
                                 constraintName="fk_item_bin_card_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="item_id"
                                 baseTableName="item_bin_card"
                                 constraintName="fk_item_bin_card_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="items"/>

    </changeSet>
</databaseChangeLog>
