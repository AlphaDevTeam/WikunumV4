<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity PaymentTypes.
    -->
    <changeSet id="20191226070353-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="payment_types"
                                 constraintName="fk_payment_types_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="invice_pay_id"
                                 baseTableName="payment_types"
                                 constraintName="fk_payment_types_invice_pay_id"
                                 referencedColumnNames="id"
                                 referencedTableName="invoice"/>

    </changeSet>
</databaseChangeLog>
