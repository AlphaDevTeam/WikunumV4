<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity CashReceiptVoucherCustomer.
    -->
    <changeSet id="20191226061656-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="history_id"
                                 baseTableName="cash_receipt_voucher_customer"
                                 constraintName="fk_cash_receipt_voucher_customer_history_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_history"/>

        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="cash_receipt_voucher_customer"
                                 constraintName="fk_cash_receipt_voucher_customer_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="transaction_type_id"
                                 baseTableName="cash_receipt_voucher_customer"
                                 constraintName="fk_cash_receipt_voucher_customer_transaction_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transaction_type"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="cash_receipt_voucher_customer"
                                 constraintName="fk_cash_receipt_voucher_customer_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>

    </changeSet>
</databaseChangeLog>
