<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity QuotationDetails.
    -->
    <changeSet id="20200101145959-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="item_id"
                                 baseTableName="quotation_details"
                                 constraintName="fk_quotation_details_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="items"/>

        <addForeignKeyConstraint baseColumnNames="quote_id"
                                 baseTableName="quotation_details"
                                 constraintName="fk_quotation_details_quote_id"
                                 referencedColumnNames="id"
                                 referencedTableName="quotation"/>

    </changeSet>
</databaseChangeLog>
