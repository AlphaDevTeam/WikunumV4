<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity SupplierAccountBalance.
    -->
    <changeSet id="20191226070342-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="supplier_id"
                                 baseTableName="supplier_account_balance"
                                 constraintName="fk_supplier_account_balance_supplier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="supplier"/>

        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="supplier_account_balance"
                                 constraintName="fk_supplier_account_balance_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="transaction_type_id"
                                 baseTableName="supplier_account_balance"
                                 constraintName="fk_supplier_account_balance_transaction_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transaction_type"/>

    </changeSet>
</databaseChangeLog>
