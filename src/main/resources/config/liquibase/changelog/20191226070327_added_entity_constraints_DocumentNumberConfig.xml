<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity DocumentNumberConfig.
    -->
    <changeSet id="20191226070327-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="history_id"
                                 baseTableName="document_number_config"
                                 constraintName="fk_document_number_config_history_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_history"/>

        <addForeignKeyConstraint baseColumnNames="document_id"
                                 baseTableName="document_number_config"
                                 constraintName="fk_document_number_config_document_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_type"/>

        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="document_number_config"
                                 constraintName="fk_document_number_config_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="transaction_type_id"
                                 baseTableName="document_number_config"
                                 constraintName="fk_document_number_config_transaction_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transaction_type"/>

    </changeSet>
</databaseChangeLog>
