<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity CashBookBalance.
    -->
    <changeSet id="20200101150020-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="cash_book_balance"
                                 constraintName="fk_cash_book_balance_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="transaction_type_id"
                                 baseTableName="cash_book_balance"
                                 constraintName="fk_cash_book_balance_transaction_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transaction_type"/>

    </changeSet>
</databaseChangeLog>
