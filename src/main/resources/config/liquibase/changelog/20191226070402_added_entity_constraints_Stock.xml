<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Stock.
    -->
    <changeSet id="20191226070402-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="history_id"
                                 baseTableName="stock"
                                 constraintName="fk_stock_history_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_history"/>

        <addForeignKeyConstraint baseColumnNames="item_id"
                                 baseTableName="stock"
                                 constraintName="fk_stock_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="items"/>

        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="stock"
                                 constraintName="fk_stock_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="stock"
                                 constraintName="fk_stock_company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"/>

    </changeSet>
</databaseChangeLog>
